{
    "name": "afiqiqmal/hashslug-laravel",
    "description": "HashSlug for Laravel Model",
    "keywords": ["package", "laravel", "hashslug", "model"],
    "type": "laravel",
    "require": {
        "php": "^7.0",
        "illuminate/config": "~5.6.0",
        "illuminate/database": "~5.6.0",
        "illuminate/support": "~5.6.0",
        "hashids/hashids": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "mockery/mockery": "^0.9.4",
        "orchestra/database": "~3.6.0",
        "orchestra/testbench": "~3.6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Hafiq",
            "email": "hafiqiqmal93@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Afiqiqmal\\LaraHashSlug\\": "src"
        },
        "files": ["helper/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Afiqiqmal\\LaraHashSlug\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Afiqiqmal\\LaraHashSlug\\LaraHashSlugProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
